# NEAR AI Cloud (from cloud.near.ai dashboard)
NEAR_AI_API_KEY=your_api_key_here

# NOVA (from nova-sdk.com)
NOVA_API_KEY=your_api_key_here
NOVA_ACCOUNT_ID=your_account.nova-sdk.near

# Database (Neon PostgreSQL)
DATABASE_URL=your_database_url_here

# Better Auth
AUTH_SECRET=your_auth_secret_here
AUTH_URL=http://localhost:3000

# NEAR Network
NEXT_PUBLIC_NEAR_NETWORK=mainnet
NEXT_PUBLIC_NEAR_RPC_URL=https://rpc.mainnet.near.org

# Smart Contract
NEXT_PUBLIC_CONTRACT_ID=ecuador5.near

# HOT Protocol (from https://pay.hot-labs.org/admin/api-keys)
NEXT_PUBLIC_HOT_API_KEY=your_hot_api_key_here
HOT_PARTNER_JWT=your_partner_jwt_here
HOT_WEBHOOK_SECRET=your_webhook_secret_here

# WalletConnect (optional - for HOT Kit)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
