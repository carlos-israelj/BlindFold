# NEAR Configuration
CONTRACT_ID=ecuador5.near
NEAR_NETWORK=mainnet

# Relayer Account (must have permissions to call contract)
RELAYER_ACCOUNT_ID=ecuador5.near
RELAYER_PRIVATE_KEY=ed25519:your_private_key_here

# NEAR AI Cloud
NEAR_AI_API_KEY=sk-your-near-ai-api-key-here
NEAR_AI_MODEL=deepseek-ai/DeepSeek-V3.1

# Polling interval (milliseconds)
POLL_INTERVAL_MS=5000

# HTTP Server Port (for health checks)
PORT=3000

# Environment
NODE_ENV=production
